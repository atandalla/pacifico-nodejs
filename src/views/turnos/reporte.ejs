<%- include ('../partials/header.ejs') %>
    <%- include ('../partials/navigation.ejs') %>
        <!-- ======= Skills Section ======= -->
        <section id="skills" class="skills section-bg">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2>REPORTE GENERAL</h2>
                    <p>El siguiente reporte presenta los turnos que han sido ingresados con su respectiva fecha</p>
                </div>

                <div class="row skills-content">

                    <div class="col-lg-12">
                        <% for(var i=0; i < products.length; i++) { %>

                            <form action="/reporte/deleteTurno/<%= products[i]._id %>?_method=DELETE" method="POST">
                                <div class="progress" style="display: inline;">
                                    <span class="skill">Cliente Caja <%= products[i].numCaja %> <button type="submit" class="btn-warning btn-sm" style="color: white;" >  Eliminar Turno</button><i class="val"><%= products[i].createdAt %> </i></span>
                                    <div class="progress-bar-wrap">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">

                                        </div>
                                    </div>

                                </div>
                            </form>
                            <% } %>
                    </div>



                </div>

            </div>
        </section>
        <!-- End Skills Section -->
        <div class="ps-pagination">
            <div class="row mt-5">
                <!-- pagination -->
                <% if(pages> 0) { %>
                    <!-- show pagination if there is pages -->
                    <nav class="mx-auto ">
                        <ul class="pagination ">
                            <!-- FIRST ITEM -->
                            <% if(current==1) { %>
                                <li class="page-item disabled ">
                                    <a class="page-link" href="#">Inicio</a>
                                </li>
                                <% } else { %>
                                    <li class="page-item">
                                        <a class="page-link" href="/reporteTurnos/1">Inicio</a>
                                    </li>
                                    <% } %>
                                        <!-- ITEMS  -->
                                        <% var i=(Number(current)> 5 ? Number(current) - 4 : 1) %>
                                            <% if(i !==1) { %>
                                                <li class="page-item disabled">
                                                    <a class="page-link" href="#">...</a>
                                                </li>
                                                <% } %>
                                                    <% for(; i <=(Number(current) + 4) && i <=pages;
                                            i++) { %>
                                                        <% if(i==current) { %>
                                                            <li class="page-item active">
                                                                <a class="page-link" href="<%= i %>">
                                                                    <%= i %>
                                                                </a>
                                                            </li>
                                                            <% } else { %>
                                                                <li class="page-item">
                                                                    <a class="page-link" href="/reporteTurnos/<%= i %>">
                                                                        <%= i %>
                                                                    </a>
                                                                </li>
                                                                <% } %>
                                                                    <% if (i==Number(current) + 4 &&
                                                            i < pages) { %>
                                                                        <li class="page-item disabled">
                                                                            <a class="page-link" href="#">...</a>
                                                                        </li>
                                                                        <% } %>
                                                                            <% } %>
                                                                                <!-- LAST ITEM -->
                                                                                <% if(current==pages)
                                                                        { %>
                                                                                    <li class="page-item disabled">
                                                                                        <a class="page-link" href="#">
                                                                                Last
                                                                            </a>
                                                                                    </li>
                                                                                    <% } else { %>
                                                                                        <li class="page-item">
                                                                                            <a class="page-link" href="/reporteTurnos/<%= pages %>">
                                                                                    Final
                                                                                </a>
                                                                                        </li>
                                                                                        <% } %>
                        </ul>
                    </nav>
                    <% } %>
            </div>
        </div>


        <%- include ('../partials/footer.ejs') %>